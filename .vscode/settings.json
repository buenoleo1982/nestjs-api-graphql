{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll.stylelint": "always"
  },
  "material-icon-theme.files.associations": {
    "*.type.ts": "graphql",
    "*.input.ts": "graphql",
    "*.args.ts": "nest-decorator",
    "*.entity.ts": "nest-pipe",
    "*.resolver.ts": "nest-controller",
    "*.service.ts": "nest-service",
    "*.module.ts": "nest-module",
    "*.use-case.ts": "nest-gateway"
  },
  "files.associations": {
    "*.ts": "typescript",
    "*.service.ts": "typescript",
    "*.module.ts": "typescript",
    "*.controller.ts": "typescript",
    "*.resolver.ts": "typescript",
    "*.entity.ts": "typescript",
    "*.input.ts": "typescript",
    "*.args.ts": "typescript",
    "*.type.ts": "typescript",
    "*.pipe.ts": "typescript",
    "*.decorator.ts": "typescript",
    "*.middleware.ts": "typescript"
  }
}
