{
  "name": "graphql",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:oracle": "typeorm-ts-node-commonjs -d src/config/typeorm.oracle.config.ts",
    "typeorm:postgres": "typeorm-ts-node-commonjs -d src/config/typeorm.postgres.config.ts",
    "typeorm:mssql": "typeorm-ts-node-commonjs -d src/config/typeorm.mssql.config.ts",
    "migration:generate:oracle": "npm run typeorm:oracle migration:generate",
    "migration:generate:postgres": "npm run typeorm:postgres migration:generate",
    "migration:generate:mssql": "npm run typeorm:mssql migration:generate",
    "migration:run:oracle": "npm run typeorm:oracle migration:run",
    "migration:run:postgres": "npm run typeorm:postgres migration:run",
    "migration:run:mssql": "npm run typeorm:mssql migration:run",
    "migration:run": "npm run migration:run:oracle && npm run migration:run:postgres && npm run migration:run:mssql",
    "migration:revert:oracle": "npm run typeorm:oracle migration:revert",
    "migration:revert:postgres": "npm run typeorm:postgres migration:revert",
    "migration:revert:mssql": "npm run typeorm:mssql migration:revert",
    "migration:revert": "npm run migration:revert:oracle && npm run migration:revert:postgres && npm run migration:revert:mssql",
    "migration:generate": "ts-node scripts/generate-migrations.ts"
  },
  "dependencies": {
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-express": "^3.13.0",
    "dotenv": "^16.4.7",
    "mssql": "^11.0.1",
    "oracledb": "^6.7.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tedious": "^18.6.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "env-cmd": "^10.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "graphql": "^16.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
